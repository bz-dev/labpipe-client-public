{
  "name": "labpipe-assistant",
  "version": "0.0.0",
  "description": "Labpipe migration work to electron based apps for cross-platform compatibility.",
  "main": "index.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mw --publish=always",
    "electron": "electron ."
  },
  "author": "Bo Zhao",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colin-bz/release-warehouse.git"
  },
  "build": {
    "appId": "com.shallowsearch.labpipe.assistant",
    "publish": {
      "provider": "github",
      "repo": "release-warehouse",
      "owner": "colin-bz",
      "vPrefixedTagName": true,
      "protocol": "https",
      "private": true,
      "releaseType": "draft"
    },
    "mac": {
      "category": "productivity",
      "target": [
        {
          "target": "zip"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "electron-debug": "^3.0.0",
    "electron-log": "^3.0.6",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.0.6",
    "nedb": "^1.8.0",
    "nedb-promises": "^3.0.6",
    "uuid": "^3.3.2"
  }
}
